text{
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #ffffff;
  background-color: #F5F5F5;
  padding-left: 2%;
  height: 90rpx;
  font-size: 35rpx;
}

.list{
  height: 50%;
  margin: 0 auto;
  overflow: scroll;
  border-bottom: 20rpx solid #F5F5F5;
}

.container{
  
  width: 100%;
  height: 100%;
  margin: 0 auto;
  overflow: scroll;
}


.file-content{
  display: flex;
  align-items: center;
  border-bottom: 10rpx solid #F5F5F5;
  padding-left: 5%;
  height: 80rpx;
  font-size: 35rpx;
}

.title{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  vertical-align: middle;
  position:relative;
  background: #f5f5f5;
  margin: 0 auto;
}

button{
  display: flex;
  position: absolute;
  right: 3%;
  background: #ffffff;
  width: 10%;
  height: 70rpx;
  justify-content: center;
  align-items: center;
  font-size: 50rpx;
  font-weight: 900;
}